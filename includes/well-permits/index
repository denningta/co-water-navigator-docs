  <span class="nl">coordsEw</span><span class="p">?:</span> <span class="kr">number</span><span class="p">;</span> <span class="c1">// Distance and direction from East/West section line (feet)</span>
  <span class="nl">coordsEwDir</span><span class="p">?:</span> <span class="kr">string</span><span class="p">;</span> <span class="c1">// Direction of measurement from East/West section line</span>
