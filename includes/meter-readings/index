<h1 id='meter-readings'>Meter Readings</h1>
<h2 id='meter-reading-properties'>Meter Reading Properties</h2>

<p>Meter readings are representative of the data found in the DBB-004 Meter Readings form</p>

<div class="highlight"><pre class="highlight typescript"><code><span class="kr">interface</span> <span class="nx">MeterReading</span> <span class="p">{</span>
  <span class="nl">date</span><span class="p">:</span> <span class="kr">string</span><span class="p">;</span>
  <span class="nl">permitNumber</span><span class="p">:</span> <span class="kr">string</span><span class="p">;</span>
  <span class="nl">flowMeter</span><span class="p">?:</span> <span class="nx">CalculatedValue</span><span class="p">;</span>
  <span class="nl">powerMeter</span><span class="p">?:</span> <span class="nx">CalculatedValue</span><span class="p">;</span>
  <span class="nl">powerConsumptionCoef</span><span class="p">?:</span> <span class="nx">CalculatedValue</span><span class="p">;</span>
  <span class="nl">pumpedThisPeriod</span><span class="p">?:</span> <span class="nx">CalculatedValue</span><span class="p">;</span>
  <span class="nl">pumpedYearToDate</span><span class="p">?:</span> <span class="nx">CalculatedValue</span><span class="p">;</span>
  <span class="nl">availableThisYear</span><span class="p">?:</span> <span class="nx">CalculatedValue</span><span class="p">;</span>
  <span class="nl">readBy</span><span class="p">?:</span> <span class="kr">string</span><span class="p">;</span>
  <span class="nl">comments</span><span class="p">?:</span> <span class="kr">string</span><span class="p">;</span>
  <span class="nl">createdAt</span><span class="p">?:</span> <span class="kr">string</span><span class="p">;</span>
  <span class="nl">createdBy</span><span class="p">?:</span> <span class="nx">UserData</span><span class="p">;</span>
  <span class="nl">updatedAt</span><span class="p">?:</span> <span class="kr">string</span><span class="p">;</span>
  <span class="nl">updatedBy</span><span class="p">?:</span> <span class="nx">UserData</span><span class="p">;</span>
<span class="p">}</span>

<span class="kr">interface</span> <span class="nx">CalculatedValue</span> <span class="p">{</span>
  <span class="nl">value</span><span class="p">:</span> <span class="kr">number</span><span class="p">;</span>
  <span class="nl">shouldBe</span><span class="p">:</span> <span class="kr">number</span> <span class="o">|</span> <span class="kc">undefined</span><span class="p">;</span>
  <span class="nl">calculationState</span><span class="p">:</span> <span class="dl">"</span><span class="s2">success</span><span class="dl">"</span> <span class="o">|</span> <span class="dl">"</span><span class="s2">warning</span><span class="dl">"</span> <span class="o">|</span> <span class="dl">"</span><span class="s2">error</span><span class="dl">"</span> <span class="o">|</span> <span class="kc">undefined</span><span class="p">;</span>
  <span class="nl">source</span><span class="p">:</span> <span class="dl">"</span><span class="s2">user</span><span class="dl">"</span> <span class="o">|</span> <span class="dl">"</span><span class="s2">calculation</span><span class="dl">"</span><span class="p">;</span>
<span class="p">}</span>
</code></pre></div>

<blockquote>
<p>Below is an example record in JSON</p>
</blockquote>

<div class="highlight"><pre class="highlight json tab-json"><code><span class="p">{</span><span class="w">
  </span><span class="nl">"acreFeetAvailableThisYear"</span><span class="p">:</span><span class="w"> </span><span class="p">{</span><span class="w">
    </span><span class="nl">"calculationShouldBe"</span><span class="p">:</span><span class="w"> </span><span class="s2">"210.00"</span><span class="p">,</span><span class="w">
    </span><span class="nl">"calculationState"</span><span class="p">:</span><span class="w"> </span><span class="s2">"success"</span><span class="p">,</span><span class="w">
    </span><span class="nl">"source"</span><span class="p">:</span><span class="w"> </span><span class="s2">"calculation"</span><span class="p">,</span><span class="w">
